<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ValueHolderView">
        <attr name="iconResource" format="reference"/>
        <attr name="holderHint" format="string"/>
    </declare-styleable>

    <declare-styleable name="RoundCornersImageView">
        <attr name="cornersRadius" format="dimension"/>
        <attr name="drawOutline" format="boolean"/>
        <attr name="outlineColor" format="color"/>
        <attr name="outlineWidth" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="DotFillerView">
        <attr name="dotSize" format="dimension"/>
        <attr name="dashSize" format="dimension"/>
        <attr name="dotColor" format="color"/>
    </declare-styleable>

</resources>